    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "type": "module",
  "version": "1.1.0",
  "wopr": {
    "plugin": {
      "category": "provider",
      "description": "OpenCode AI SDK for coding tasks",
      "name": "provider-opencode"
    }
